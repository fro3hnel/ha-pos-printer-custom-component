# Minimal Raspberry Pi OS Lite image for Raspberry Pi Zero W
IMG_NAME='pos-printer-zero-w'
RELEASE='bookworm'
ARCH='armhf'
DEPLOY_COMPRESSION='xz'

ENABLE_SSH=1
DISABLE_FIRST_BOOT_USER_RENAME=1
TARGET_HOSTNAME='pos-printer'

LOCALE_DEFAULT='en_US.UTF-8'
KEYBOARD_KEYMAP='us'
KEYBOARD_LAYOUT='English (US)'
TIMEZONE_DEFAULT='Etc/UTC'

STAGE_LIST="stage0 stage1 stage2 stage-pos-printer"
